func isPrime(num) {
  for var i = 2; i < num; i++; {
    if num % i == 0 {
      return false;
    }
  }
  return true;
}

func nthPrime(n){
  var i = 0;
  var num = 1;
  var b = false;
  for {
    var a = isPrime(num);
    if a {
      i += 1;

      if i > n {
        break;
      }

    }
    num +=1;
  }
  return num;
}


func printPrimes(n) {
  for var i =1; i<= n;i++; {
    var num = nthPrime(i);
    print(num);
  }
}

printPrimes(20);