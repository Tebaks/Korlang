
func randInt(start,end) {
  var rawRand = random();
  var size = end - start;
  var r = rawRand % size;
  r += start;
  return r;
}

func randFloat(start,end){
  var rawRand = float(random());
  var size = end - start;
  var r = rawRand % size;
  r += start;
  return r;
}
 



extract randInt;
extract randFloat;