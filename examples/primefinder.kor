
print("Please enter -1 to exit.");


func isPrime(num){
  for var i = 2;i<num;i++;{
    if num % i == 0 {
      print(num,i,"son");
      return false;
    }
  }
  return true;
}


for {
  print();
  var n = int(input("Enter a number: "));
  
  if n < 0 {
    break;
  }
  
  var isp = isPrime(n);
  if isp {
    print("Prime number");
  } else {
    print("Not a prime number");
  }
  
  

}
